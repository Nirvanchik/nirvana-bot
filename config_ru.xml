<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<comment>no comments</comment>
<entry key="out-dir">out/ru</entry>
<entry key="log4j-settings">config/log4j-log.properties</entry>
<entry key="wiki-login">NirvanaBot</entry>
<entry key="wiki-password"></entry>
<entry key="wiki-account-file">loginNirvanaBot.xml</entry>
<entry key="wiki-maxlag">5</entry>
<entry key="new-pages-template">
	Участник:NirvanaBot/Новые статьи,
	Участник:ClaymoreBot/Новые статьи
</entry>
<entry key="overriden-properties-page">Участник:NirvanaBot/Новые статьи/Настройки по умолчанию</entry>
<entry key="wiki-lang">ru</entry>
<entry key="default-depth">6</entry>
<entry key="default-hours">350</entry>
<entry key="default-maxitems">20</entry>
<entry key="picture-search-tags">image file,Фото,портрет,Изображение</entry>
<entry key="fair-use-image-templates">Обоснование добросовестного использования, Disputed-fairuse, ОДИ, Несвободный файл/ОДИ</entry>
<entry key="discussion-pages-templates">
	К удалению = Википедия:К удалению/%(дата)#%(название), к удалению
	К переименованию = Википедия:К переименованию/%(дата)#%(текст_с_названием), к переименованию
	К объединению = Википедия:К объединению/%(дата)#%(текст_с_названием), к объединению
	К улучшению = Википедия:К улучшению/%(дата)#%(название), к улучшению
</entry>
<entry key="discussion-pages-settings-wikipage">Участник:NirvanaBot/Новые статьи/Настройки обсуждаемых статей.js</entry>
<entry key="error-notification">yes</entry>
<entry key="update-pause">1000</entry>
<entry key="statistics">yes</entry>
<entry key="statistics-file">report/report.NirvanaBot.ru_%(date)_%(launch_number).txt</entry>
<entry key="statistics-format">txt,wiki</entry>
<entry key="statistics-wiki-verbosity">verbose</entry>
<entry key="update-status">yes</entry>
<entry key="statistics-wiki">Участник:NirvanaBot/Новые статьи/Отчёт</entry>
<entry key="status-wiki">Участник:NirvanaBot/Новые статьи/Статус</entry>
<entry key="status-wiki-template">Участник:NirvanaBot/Новые статьи/Отображение статуса</entry>
<entry key="start-from">0</entry>
<entry key="default-service">petscan</entry>
<entry key="service">auto</entry>
<entry key="fast-mode">yes</entry>
<entry key="long-task">yes</entry>
<entry key="bot-timeout">86400000</entry>
<entry key="services-timeout">36000000</entry>
<entry key="try-count">2</entry>
<entry key="catscan-try-count">3</entry>
</properties>
